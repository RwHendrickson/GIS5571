% Reassessing Disparities in EJ Research

@article{mohai2006,
	author = {Mohai, Paul and Saha, Robin},
	year = {2006},
	month = {06},
	pages = {383-99},
	title = {Reassessing Race and Socioeconomic Disparities in Environmental Justice Research},
	volume = {43},
	journal = {Demography},
	doi = {10.1353/dem.2006.0017},
	annotation = {\\
	This paper discusses different methods and shortcomings in assessing racial and socio-economics as they relate to environmental hazards. The most popular method in these analyses has historically been the unit-hazard approach which defines a unit that hosts an environmental hazard (typically a zip code or county) and looks at demographics within that unit. This fails to acccount for the actual location of the site. The authors argue for distance-based models as an alternative and discuss methods to conduct this analysis. They then demonstrate that disparities are more pronounced when using distance-based models and insist that, as GIS technology advances, these methods will become easier to conduct. The paper is a bit of an artifact for modern GIS users because we are now very familiar with distance-based models. Still, it provokes one to reconsider statements made in earlier research and be critical of papers that do not adopt these types of analyses.
	}
	}

